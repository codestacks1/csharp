<Window x:Class="Xiaowen.Windows.Video.Views.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:views="clr-namespace:Xiaowen.Windows.Video.Views"
        xmlns:local="clr-namespace:Xiaowen.Windows.Video"
        mc:Ignorable="d"
        Title="MainWindow" Height="589" Width="1036.525" 
        WindowStyle="None" WindowStartupLocation="CenterScreen" 
        Background="{x:Null}" Foreground="{x:Null}" 
        AllowsTransparency="True">

    <Grid>
        <local:Loading Margin="-33,0,-32,-16" 
                       Foreground="{x:Null}" 
                       DataContext="{Binding}" 
                       Loaded="Loading_Loaded">
        </local:Loading>
    </Grid>
</Window>
