<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:VideoAnalysis"
        xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core" x:Class="VideoAnalysis.HomeWindow"
        mc:Ignorable="d"
        Title="Home" Height="589" Width="1036.525"
        WindowStartupLocation="CenterScreen" Icon="/img/logo_01.ico">
    <Grid>

        <dx:DXTabControl>
            <dx:DXTabItem Header="Tab0">
                <Grid Background="Transparent" DataContext="{Binding}">

                    <Button x:Name="ViewMap" Click="ViewMap_Click" Content="Button01"></Button>

                </Grid>
            </dx:DXTabItem>
            <dx:DXTabItem Header="Tab1">
                <Grid Background="Transparent" DataContext="{Binding}"/>
            </dx:DXTabItem>
            <dx:DXTabItem Header="Tab2">
                <Grid Background="Transparent" DataContext="{Binding}"/>
            </dx:DXTabItem>
        </dx:DXTabControl>

    </Grid>
</Window>
